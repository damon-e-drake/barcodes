<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>Damon E Drake</Authors>
    <PackageProjectUrl>https://github.com/damon-e-drake/barcodes</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/damon-e-drake/barcodes</RepositoryUrl>
    <PackageTags>barcodes, code39, ean, bookland, isbn</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>1.0.2-alpha</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
  </ItemGroup>

</Project>
